{
    "version": 2,
    "builds": [
        {
            "src": "api.py",
            "use": "@vercel/python",
            "config": {
                "buildCommand": "pip install -r requirements.txt && playwright install chromium --with-deps"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api.py"
        }
    ],
    "functions": {
        "api.py": {
            "memory": 1024,
            "maxDuration": 10
        }
    }
} 